#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;//for test case
    cout<<"Entre no of test case:"<<"\n";
    cin>>t;
	int no_of_topic=10;//No of topics
	int n,x;
	int prepared[no_of_topic];

    for(int i=1;i<=t;i++){
    cout<<"\nTest case "<<i<<"\n";    
	cout<<"No of topic that she had prepared:"<<"\n";
	cin>>n;
    cout<<"Enter the topic chef have prepared:"<<"\n";

	for(int i=0;i<n;i++){
	cin>>prepared[i];
	}
    
	cout<<"Enter the topic that chef get to speak:"<<"\n";
	cin>>x;
	for(int i=0;i<n;i++){
	    if(prepared[i]==x){
	        cout<<"Yes, chef has chance to win:"<<"\n";
            break;
	    }else{
	        cout<<"No, chef have no chance to win:"<<"\n";
            break;
	    }
	}
    }
return 0;
}
