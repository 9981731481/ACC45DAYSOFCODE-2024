#CHEF AND STEPS
T=int(input('Enter the no of test case:'))
for i in range(1,T+1):
    print('Test case:',i)
    N=int(input('Enter the no of integer N='))
    k=int(input('Enter the distance K=:'))
    num=input('Enter the integers:').split()
    num=list(int(num[i]) for i in range(N))
    for n in num:
        if n%k==0:
            print(1,end='')
        else:
            print(0,end='')
    print('\n')
