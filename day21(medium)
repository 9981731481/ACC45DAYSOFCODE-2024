#IPL and RCB
def min_wins():
    x=int(input('Enter the number of points:'))
    y=int(input('Enter the number of matches:'))
    min_win=max(0,x-y)
    return min_win
t=int(input('Enter the number of test case:'))
for i in range(1,t+1):
    print('Test case:',i)
    print('No of matches=',min_wins())



