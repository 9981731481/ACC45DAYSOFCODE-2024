#to find the index of the first occurance in a string
def findindex(haystack,needle):
    indexing=haystack.find(needle)
    print('Index=',indexing)

t=int(input('Enter the number of test case:'))
for i in range(1,t+1):
    print('For test case:',i)
    haystack=input('Enter the string haystack=')
    needle=input('Enter the string needle=')
    findindex(haystack,needle)
