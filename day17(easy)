#car or bike
def timetaken():
    t_car=int(input('Time taken by the car to reach the home(IN MIN)='))
    t_bike=int(input('Time taken by the bike to reach the home(IN MIN)='))

    if t_bike>t_car:
        print("Bike is faster the car:")
    elif t_bike<t_car:
        print("Car is faster the bike:")
    else:
        print("Same timetaken by both car and bike:")

t=int(input('Enter the test case:'))
for i in range(1,t+1):
    print('Test case',i)
    timetaken()
