#dracula eats
def dracula_eats():
    N=int(input('Enter the number of days'))
    days=['monday','tuesday','wednesday','thursday','friday','saturday','sunday']
    n_days=len(days)
    time=0
    for i in range(N):
        day=(i)%n_days
        if days[day]=='tuesday':
            time+=1
    print(time)

t=int(input('Enter the test case:'))
for i in range(1,t+1):
    print('test case',i)
    dracula_eats()
