#policeman and thef
def timetaken():
    x=int(input('Initial position of policeman:'))
    y=int(input('Initial position of thef'))

    u_x=2 #initial velocity of policeman is 2 unit per second
    u_y=1 #initial velocity of thef is 2 unit per second
    relat_t=abs((x-y)/(u_x-u_y))

    print(f"minimum time {relat_t} second")

t=int(input('Enter the number of test case:'))
for i in range(1,t+1):
    print('Test case:',i)
    timetaken()
