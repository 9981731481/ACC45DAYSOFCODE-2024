#to check the quadilateral or not
t=int(input('Enter the test case:'))
for i in range(1,t+1):
    print('Test case:',i)
    angles=input('Enter the angles:').split()
    angles=list(int(angles[i]) for i in range(0,4))
    if angles[0]+angles[2]==180 and angles[1]+angles[3]==180:
        print('Yes,it is quadrilateral')
    else:
        print('No')
