#maximise the tastiness
def tastiness():
    a,b,c,d=input('Enter the tastiness of ingredient A B C D =').split()
    a,b,c,d=int(a),int(b),int(c),int(d)
    max_tastiness=max(a,b)+max(c,d)
    print('Maximum testiness of dish',max_tastiness,end='\n')

t=int(input('Enter the test case:'))
for i in range(1,t+1):
    print('Test case:',i)
    tastiness()
