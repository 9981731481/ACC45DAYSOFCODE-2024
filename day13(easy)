def expert_setter():
    x=int(input('Enter the total number of problem submitted:'))
    y=int(input('Enter the number of problem which is approved:'))
    if y>=(x*50)/100:
        print('Yes,munchy is expert:')
    else:
        print('No,munchy is not expert')


t=int(input('Enter the test case:'))
for i in range(1,t+1):
    print('Test case:',i)
    expert_setter()
